/keymaster             emmc     /dev/block/bootdevice/by-name/keymaster             flags=display="Keymaster";slotselect
/oplus_sec             emmc     /dev/block/bootdevice/by-name/oplus_sec             flags=display="Oplus_sec";slotselect
/oplusstanvbk          emmc     /dev/block/bootdevice/by-name/oplusstanvbk          flags=display="Oplusstanvbk";slotselect
/qupfw                 emmc     /dev/block/bootdevice/by-name/qupfw                 flags=display="Qupfw";slotselect
/shrm                  emmc     /dev/block/bootdevice/by-name/shrm                  flags=display="Shrm";slotselect
/splash                emmc     /dev/block/bootdevice/by-name/splash                flags=display="Splash";slotselect
/tz                    emmc     /dev/block/bootdevice/by-name/tz                    flags=display="Tz";slotselect
